#include <Wire.h>
#include <SPI.h>
#include <TPush.h>
#include "RTClib.h"
#include "OneButton.h"
#include <Adafruit_GFX.h>
#include <Adafruit_PCD8544.h>
#define TEA5767_mute_left_right  0x06
#define TEA5767_MUTE_FULL        0x80
#define TEA5767_ADC_LEVEL_MASK   0xF0
#define TEA5767_STEREO_MASK      0x80
#define lcd_luminosity             3
#define button_back               10
#define button_frequency_down     11
#define button_frequency_up       12
#define button_menu               13
OneButton button(12, true);


Adafruit_PCD8544 lcd = Adafruit_PCD8544(4, 5, 6, 7, 8);
RTC_DS1307 RTC;

unsigned long upButton;
unsigned long upTimeButton = 450;
bool upReady = false;
bool upState = false;

byte old_stereo = 0;
byte stereo = 1;
byte old_mute = 1;
byte mute = 0;
byte old_signal_level = 1;
byte signal_level = 0;

int frequency = 10140; //Frequenza iniziale
int old_frequency = -1;
int boot = 0;
int p = 1;
int vmenu = 0;
int part = 0;
int step = 0;
int smenu = 0;
int c = 0;
int contr = 25; //30




String space = "|";
String acceso = "On";
String interruttore = String (space + acceso);
String numero = String (space + contr);



void setup(void) {

  pinMode(button_back, INPUT);
  pinMode(button_frequency_down, INPUT);
  pinMode(button_frequency_up, INPUT);
  pinMode(button_menu, INPUT);
  pinMode(lcd_luminosity, OUTPUT);




  start();
  boot = 1;

  Serial.begin(9600);
  Wire.begin();
  RTC.begin();
  //Per impostare la data per la prima volta nell'orologio togliere il commento '//' sotto
  RTC.adjust(DateTime(__DATE__, __TIME__));



}

void loop() {
  DateTime now = RTC.now();


  if ((now.hour(), DEC) <= 8) {
    digitalWrite(lcd_luminosity, HIGH);
  }
  else if ((now.hour(), DEC) >= 12) {
    digitalWrite(lcd_luminosity, LOW);
  }




  TEA5767_read_data();
  if (frequency != old_frequency && p == 1) {
    set_text(old_frequency >= 10000 ? 6 : 14, 17, value_to_string(old_frequency), WHITE, 2);
    set_text(frequency >= 10000 ? 6 : 14, 17, value_to_string(frequency), BLACK, 2);
    old_frequency = frequency;
  }

  if (old_stereo != stereo && p == 1) {
    set_text(old_stereo ? 22 : 28, 39, old_stereo ? "Stereo" : "Mono", WHITE, 1);
    set_text(stereo ? 22 : 28, 39, stereo ? "Stereo" : "Mono", BLACK, 1);
    old_stereo = stereo;
  }

  if (old_signal_level != signal_level) {
    set_text(old_signal_level < 10 ? 76 : 70, 39, String((int)old_signal_level), WHITE, 1);
    set_text(signal_level < 10 ? 76 : 70, 39, String((int)signal_level), BLACK, 1);
    old_signal_level = signal_level;
    show_signal_level(signal_level);
  }

  if (old_mute != mute) {
    set_text(1, 39, old_mute ? "M" : "S", WHITE, 1);
    set_text(1, 39, mute ? "M" : "S", BLACK, 1);
    old_mute = mute;
  }

  //---- Abbassare la frequenza
  if (digitalRead(button_frequency_down) == HIGH && p == 1) {
    frequency = frequency - 5;
    if (frequency < 8750)frequency = 10800;
    TEA5767_set_frequency();
  }

  //---- Aumentare la frequenza
  else if (digitalRead(button_frequency_up) == HIGH && p == 1) {

    frequency = frequency + 5;

    if (frequency > 10800)frequency = 8750;
    TEA5767_set_frequency();
  }


  //---- Entrare nel menù
  else if (digitalRead(button_menu) == HIGH && p == 1) {
    if (p == 1) {
      p = 0;
      vmenu = 1;
      smenu = 0;
    }
    int o;
    c = 1;
    print();
    for (o = 1; o < 100; o++) {}
    vmenu == 1;
    step = 1;
    delay(200);
  }

  //---- Tasto Back nel menù
  else if (vmenu >= 1 && part >= 0 && smenu == 0 && digitalRead(button_back) == HIGH) {
    vmenu = 10; part = 10;
    lcd.clearDisplay();
    lcd.display();
    software_Reboot();
    smenu = 1;
  }

  //---- Freccia destra nel menù luminosità
  else if (vmenu == 1 && part == 0 && digitalRead(button_frequency_up) == HIGH) {
    vmenu = 10; part = 10;
    digitalWrite(lcd_luminosity, HIGH);
    blink();
    lcd.clearDisplay();
    lcd.drawBitmap(0, 0,  menu, 84, 48, 1);
    station();
    old_frequency = frequency;
    set_text( 57, 10, acceso, BLACK, 1);
    set_text( 57, 20, space + contr, BLACK, 1);
    vmenu = 2;
    part = 1;
    step = 1;
    delay(80);
  }

  //---- Freccia sinistra nel menù luminosità
  else if (vmenu == 2 && part == 1 && digitalRead(button_frequency_down) == HIGH) {
    vmenu = 10; part = 10;
    digitalWrite(lcd_luminosity, LOW);
    blink();
    lcd.clearDisplay();
    lcd.drawBitmap(0, 0,  menu, 84, 48, 1);
    station();
    old_frequency = frequency;
    set_text( 57, 10, interruttore, BLACK, 1);
    set_text( 57, 20, space + contr, BLACK, 1);
    vmenu = 1;
    part = 0;
    step = 1;
    delay(80);
  }

  //---- Freccia destra nel menù contrasto
  else if (vmenu == 3 && part == 2 && digitalRead(button_frequency_up) == HIGH) {
    vmenu = 10; part = 10;
    lcd.clearDisplay();
    lcd.drawBitmap(0, 0,  menu3, 84, 48, 1);
    station();
    old_frequency = frequency;
    lcd.begin();
    if (contr < 80) {
      contr = contr + 5;
      lcd.setContrast(contr);
    }
    set_text( 57, 10, space + contr, BLACK, 1);
    vmenu = 3;
    part = 2;
    step = 2;

    delay(200);

  }
  //---- Freccia sinistra nel menù contrasto
  else if (vmenu == 3 && part == 2 && digitalRead(button_frequency_down) == HIGH) {
    vmenu = 10; part = 10;
    lcd.clearDisplay();
    lcd.drawBitmap(0, 0,  menu3, 84, 48, 1);
    station();
    lcd.begin();
    if (contr > 25) {
      contr = contr - 5;
      lcd.setContrast(contr);
    }
    set_text( 57, 10, space + contr, BLACK, 1);
    vmenu = 3;
    part = 2;
    delay(200);
  }

  //---- Tasto Ok nel menù, prima riga
  else if (step == 1 && digitalRead(button_menu) == HIGH) {
    vmenu = 3;
    part = 3;
    step = 6;
    lcd.clearDisplay();
    lcd.drawBitmap(0, 0,  menu3, 84, 48, 1);
    station();
    old_frequency = frequency;
    set_text( 57, 10, space + contr, BLACK, 1);
    step = 2;
    vmenu = 3;
    part = 2;
    delay(80);
  }

  //---- Tasto Ok nel menù, Seconda riga
  if (step == 2 && digitalRead(button_menu) == HIGH) {
    vmenu = 10; part = 10;
    lcd.clearDisplay();

    blink();
    lcd.drawBitmap(0, 0,  menu, 84, 48, 1);
    station();
    set_text( 57, 10, acceso, BLACK, 1);
    set_text( 57, 20, space + contr, BLACK, 1);
    vmenu = 1;
    part = 0;
    step = 1;
    delay(80);
  }

}
